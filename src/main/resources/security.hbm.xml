<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="tw.com.msig.entity.Security" table="my_security">
        <id name="id" column="id" type="long">
            <generator class="identity" />
        </id>
        <property name="employeeId" column="employee_id" type="string"/>
        <property name="group" column="`group`" type="string"/>
        <property name="enabled" column="enabled" type="string"/>
                
    </class>
</hibernate-mapping>