<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="tw.com.msig.entity.Attribute" table="my_attribute">
		<id name="id" column="id" type="long">
			<generator class="sequence" />
		</id>

		<property name="attType" column="att_type" type="string" />
		<property name="attValue" column="att_value" type="string" />
		<property name="attComm" column="att_comm" type="string" />

	</class>
</hibernate-mapping>